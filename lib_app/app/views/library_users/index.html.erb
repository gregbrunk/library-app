<div>
	<h2><%= @user.first_name %>'s Joined Libraries:</h2>
</div>
<% @libraries.each do |library| %>
    <h3><%= library.name %></h3>
    <p style="display: inline-block">Floor Count: <%= library.floor_count %></p>
    &nbsp&nbsp|&nbsp&nbsp
    <p style="display: inline-block">Floor Area: <%= library.floor_area %></p>
    <br>
<% end %>
<br>
<%= button_to 'Back to User', user_path(@user), method: :get %>
